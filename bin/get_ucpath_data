#!/bin/sh

# Get voyager environment, for vars and for cron
. `echo $HOME | sed "s/$LOGNAME/voyager/"`/.profile.local

DIR=/m1/voyager/ucladb/local/patron

JAVA=${BIN}/java
JAVADIR=${VGER_JAVA}
CLASSPATH=${JAVADIR}/log4j-1.2.17.jar:${JAVADIR}/commons-logging-1.2.jar:${JAVADIR}/commons-collections-3.1.jar:${JAVADIR}/commons-digester-1.7.jar:${JAVADIR}/xml-apis-1.0.b2.jar:${JAVADIR}/commons-beanutils-1.6.jar:${JAVADIR}/spring-jdbc-4.1.6.RELEASE.jar:${JAVADIR}/spring-beans-4.1.6.RELEASE.jar:${JAVADIR}/spring-core-4.1.6.RELEASE.jar:${JAVADIR}/spring-tx-4.1.6.RELEASE.jar:${JAVADIR}/sqljdbc.jar:${JAVADIR}/ucpath.jar
PROPERTIES_FILE=${JAVADIR}/ucpath.props

echo "starting UCPath extract at `date`"
${JAVA} -classpath ${CLASSPATH} edu.ucla.library.libservices.ucpath.main.Loader ${PROPERTIES_FILE}
echo "ending UCPath extract at `date`"
