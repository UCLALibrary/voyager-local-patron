#!/bin/sh

# Get voyager environment, for vars and for cron
. `echo $HOME | sed "s/$LOGNAME/voyager/"`/.profile.local

DIR=/m1/voyager/ucladb/local/patron

JAVA=/m1/shared/java/jdk1.8.0_131/bin/java
JAVADIR=${VGER_JAVA}
CLASSPATH=${JAVADIR}/log4j-api-2.11.2.jar:${JAVADIR}/log4j-core-2.11.2.jar:${JAVADIR}/commons-logging-1.2.jar:${JAVADIR}/commons-collections-3.1.jar:${JAVADIR}/commons-digester-1.7.jar:${JAVADIR}/xml-apis-1.0.b2.jar:${JAVADIR}/commons-beanutils-1.6.jar:${JAVADIR}/spring-jdbc-5.1.6.RELEASE.jar:${JAVADIR}/spring-beans-5.1.6.RELEASE.jar:${JAVADIR}/spring-core-5.1.6.RELEASE.jar:${JAVADIR}/spring-tx-5.1.6.RELEASE.jar:${JAVADIR}/spring-jcl-5.1.6.RELEASE.jar:${JAVADIR}/mssql-jdbc-8.2.2.jre8.jar:${JAVADIR}/ucpath.jar
PROPERTIES_FILE=${JAVADIR}/ucpath.props

echo "starting UCPath extract at `date`"
${JAVA} -classpath ${CLASSPATH} edu.ucla.library.libservices.ucpath.main.Loader ${PROPERTIES_FILE}
echo -e "\nending UCPath extract at `date`"
